    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "lodash.merge": "^4.6.2",
    "npm-check-updates": "^12.5.3",
    "nunjucks": "^3.2.3",
    "pg-promise": "^10.11.1",
