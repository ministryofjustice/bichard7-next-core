{
  "name": "moj-bichard7",
  "private": true,
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand --forceExit --testNamePattern=",
    "test:unit": "jest --maxWorkers=2 --testPathIgnorePatterns='comparison.test|integration.test|e2e.test' --testNamePattern=",
    "test:update": "jest -u --testPathIgnorePatterns='comparison.test|integration.test' 'integration.test'",
    "test:bichard": "USE_BICHARD=true jest --runInBand --testPathIgnorePatterns='comparison.test|integration.test' --testNamePattern=",
    "test:watch": "jest --watch --testPathIgnorePatterns='comparison.test|integration.test'",
    "test:compare": "NODE_ENV=test ts-node -T src/comparison/cli.ts -d ./test-data/e2e-comparison",
    "test:compare:e2e": "jest --runInBand --forceExit --testPathPattern=e2e.comparison.test",
    "test:integration": "jest --forceExit --runInBand --testPathPattern=integration.test",
    "test:conductor": "jest --forceExit --runInBand --testPathPattern=conductor",
    "test:server": "NODE_ENV=test ts-node scripts/testServer.ts",
    "test:e2e": "docker-compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-worker.yml -f environment/docker-compose-e2e-tests.yml run e2e-tests npm run test:chunk",
    "test:e2e:ci": "docker-compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-e2e-tests.yml run e2e-tests npm run test:chunk",
    "compare": "TS_NODE_TRANSPILE_ONLY=true ts-node src/comparison/cli.ts",
    "compare:files": "scripts/files-from-comparison.js",
    "compare:summarise": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/summariseRecord.ts",
    "compare:summarise:extended": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/extendedSummary.ts",
    "compare:repeat": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/repeatTestBichard.ts",
    "compare:skip": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/skip-test.ts",
    "compare:runonbichard": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/runFileOnBichard.ts",
    "build": "node build.js",
    "build:package": "./scripts/build-package.sh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "update-deps": "ncu -u && npm install",
    "install-standing-data": "scripts/install-standing-data.sh",
    "all": "./environment/boot.sh",
    "all-no-worker": "NOWORKER=true ./environment/boot.sh",
    "all-legacy": "LEGACY=true ./environment/boot.sh nginx-auth-proxy",
    "postgres": "docker-compose --project-name bichard -f environment/docker-compose.yml up -d --wait postgres",
    "activemq": "docker-compose --project-name bichard -f environment/docker-compose.yml up -d --wait activemq",
    "conductor": "./environment/boot.sh conductor",
    "conductor-no-worker": "NOWORKER=true ./environment/boot.sh conductor",
    "bichard-legacy": "docker-compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-bichard.yml up -d --wait nginx-auth-proxy",
    "bichard-legacy-debug": "docker-compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-bichard-debug.yml up -d --wait nginx-auth-proxy",
    "destroy": "docker-compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-worker.yml down -v",
    "conductor-setup": "GIT_COMMIT_HASH=$(git rev-parse --short HEAD) npx ts-node -T conductor/deploy/deploy.ts",
    "conductor-worker": ". ./environment/local-conductor-worker.env &&  npm run conductor-worker-noenv",
    "conductor-worker-noenv": "npx ts-node -T conductor/src/worker.ts",
    "build-worker": "docker build -f conductor/Dockerfile -t core-worker:latest .",
    "phase1": "DB_PORT=${DB_PORT:-5432} AUDIT_LOG_API_URL=${AUDIT_LOG_API_URL:-http://localhost:3010}  AUDIT_LOG_API_KEY=${AUDIT_LOG_API_KEY:-xxx} AWS_ACCESS_KEY_ID=S3RVER AWS_SECRET_ACCESS_KEY=S3RVER S3_ENDPOINT=http://localhost:4570 MQ_USER=${MQ_USER:-admin} MQ_PASSWORD=${MQ_PASSWORD:-admin} MQ_URL=${MQ_URL:-stomp://localhost:61613} PHASE_1_BUCKET_NAME=phase1 ts-node -T scripts/phase1.ts",
    "structurizr-site-generatr-build": "docker-compose -f docs/docker-compose-arch.yml run --rm structurizr-site-generatr-build",
    "structurizr-site-generatr-serve": "docker-compose -f docs/docker-compose-arch.yml run --rm --service-ports structurizr-site-generatr-serve",
    "structurizr-lite": "docker-compose -f docs/docker-compose-arch.yml run --rm --service-ports structurizr-lite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-core.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-core/issues"
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-core#readme",
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0"
  }
}