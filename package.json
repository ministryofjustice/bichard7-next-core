{
  "name": "moj-bichard7",
  "version": "2.0.9",
  "scripts": {
    "activemq": "NOWORKER=true SKIP_CONDUCTOR_SETUP=true ./environment/boot.sh activemq",
    "audit-log": "NOWORKER=true SKIP_CONDUCTOR_SETUP=true ./environment/boot.sh audit-log-api",
    "all": "./environment/boot.sh",
    "all-phase2-core": "PHASE_2_QUEUE_NAME=TEST_PHASE2_QUEUE BUILD_WORKER=true ./environment/boot.sh",
    "all-phase3-core": "PHASE_2_QUEUE_NAME=TEST_PHASE2_QUEUE PHASE_3_QUEUE_NAME=TEST_PHASE3_QUEUE BUILD_WORKER=true ./environment/boot.sh",
    "all-legacy": "PHASE2_CORE_CANARY_RATIO=0 PHASE3_CORE_CANARY_RATIO=0 LEGACY=true ./environment/boot.sh",
    "all-no-worker": "NOWORKER=true ./environment/boot.sh",
    "all-build-worker": "BUILD_WORKER=true ./environment/boot.sh",
    "bichard-legacy": "docker compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-legacy-bichard.yml up -d --wait nginx-auth-proxy",
    "bichard-legacy:phase-1": "ENABLE_PHASE_2=false ENABLE_PHASE_3=false npm run bichard-legacy",
    "bichard-legacy:phase-2": "ENABLE_PHASE_2=true ENABLE_PHASE_3=false npm run bichard-legacy",
    "bichard-legacy:phase-3": "ENABLE_PHASE_2=false ENABLE_PHASE_3=true npm run bichard-legacy",
    "bichard-legacy-debug": "docker compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-legacy-bichard-debug.yml up -d --wait nginx-auth-proxy",
    "bichard-legacy-debug:phase-1": "ENABLE_PHASE_2=false ENABLE_PHASE_3=false npm run bichard-legacy-debug",
    "bichard-legacy-debug:phase-2": "ENABLE_PHASE_2=true ENABLE_PHASE_3=false npm run bichard-legacy-debug",
    "bichard-legacy-debug:phase-3": "ENABLE_PHASE_2=false ENABLE_PHASE_3=true npm run bichard-legacy-debug",
    "build": "npm run build --ws",
    "build:core": "npm run build -w packages/common && npm run build -w packages/core && npm run build -w packages/conductor && npm run build -w packages/message-forwarder",
    "build:api": "npm run build -w packages/common && npm run build -w packages/api",
    "build:ui": "npm run build -w packages/common && npm run build -w packages/core && npm run build -w packages/ui",
    "build:user-service": "npm run build -w packages/user-service",
    "build:e2e-test": "npm run build -w packages/common && npm run build -w packages/core && npm run build -w packages/e2e-test",
    "build-message-forwarder:phase-1": "docker compose --project-name bichard -f environment/docker-compose.yml build phase-1-message-forwarder",
    "build-message-forwarder:phase-2": "docker compose --project-name bichard -f environment/docker-compose.yml build phase-2-message-forwarder",
    "build-worker": "docker compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-worker.yml build worker",
    "clean": "npm run clean --ws",
    "conductor": "./environment/boot.sh conductor audit-log-api",
    "conductor-no-worker": "NOWORKER=true ./environment/boot.sh conductor audit-log-api pnc",
    "conductor-build-worker": "BUILD_WORKER=true ./environment/boot.sh conductor audit-log-api pnc",
    "conductor-only": "NOWORKER=true ./environment/boot.sh conductor",
    "destroy": "docker compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-worker.yml down -v",
    "dev-sgs": "./scripts/dev-sgs.sh",
    "generate-schema-doc": "./scripts/generate-schema-doc.sh",
    "lint": "npm run lint --ws",
    "lint:fix": "npm run lint:fix --ws",
    "lint:ci": "bash ./scripts/lint-ci.sh",
    "postgres": "NOWORKER=true SKIP_CONDUCTOR_SETUP=true ./environment/boot.sh db-seed",
    "localstack": "NOWORKER=true SKIP_CONDUCTOR_SETUP=true ./environment/boot.sh localstack",
    "mailserver": "NOWORKER=true SKIP_CONDUCTOR_SETUP=true ./environment/boot.sh mail",
    "structurizr-lite": "docker compose -f docs/docker-compose-arch.yml run --rm --service-ports structurizr-lite",
    "structurizr-site-generatr-build": "docker compose -f docs/docker-compose-arch.yml run --rm structurizr-site-generatr-build",
    "structurizr-site-generatr-serve": "docker compose -f docs/docker-compose-arch.yml run --rm --service-ports structurizr-site-generatr-serve",
    "update-deps": "ncu -u --deep && npm install",
    "ui": "NOWORKER=true SKIP_CONDUCTOR_SETUP=true ./environment/boot.sh activemq ui && docker compose --project-name bichard -f environment/docker-compose.yml up -d --no-deps --wait nginx-auth-proxy user-service audit-log-api",
    "api": "NOWORKER=true SKIP_CONDUCTOR_SETUP=true ./environment/boot.sh api",
    "watch": "tsc --build --watch",
    "prepare": "husky"
  },
  "workspaces": [
    "packages/common",
    "packages/core",
    "packages/conductor",
    "packages/api",
    "packages/message-forwarder",
    "packages/ui",
    "packages/e2e-test",
    "packages/uat-data",
    "packages/cli",
    "packages/user-service"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ministryofjustice/bichard7-next-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-core.git"
  },
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-core/issues"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.943.0",
    "@aws-sdk/client-lambda": "^3.945.0",
    "@aws-sdk/client-rds": "^3.943.0",
    "@aws-sdk/lib-dynamodb": "^3.943.0",
    "@eslint/compat": "^2.0.0",
    "@swc/core": "^1.15.3",
    "@swc/jest": "^0.2.39",
    "@next/eslint-plugin-next": "^16.0.7",
    "@types/jest": "30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.7",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mocha": "^11.2.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "excel4node": "^1.8.2",
    "husky": "^9.1.7",
    "jest": "30.2.0",
    "lint-staged": "^16.2.7",
    "npm-check-updates": "^19.1.2",
    "zod": "^4.1.13"
  },
  "dependencies": {}
}
