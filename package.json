{
  "name": "bichard7-next-core",
  "version": "1.0.0",
  "description": "The code to replace the processing logic of Bichard 7",
  "main": "build/index.js",
  "scripts": {
    "test": "jest --maxWorkers=2 --testPathIgnorePatterns='comparison' --testNamePattern=",
    "test:update": "jest -u --testPathIgnorePatterns='comparison'",
    "test:bichard": "USE_BICHARD=true jest --runInBand --testPathIgnorePatterns='comparison' --testNamePattern=",
    "test:watch": "jest --watch --testPathIgnorePatterns='comparison'",
    "test:compare": "jest --testPathPattern=comparison",
    "clean": "rimraf build",
    "compare": "ts-node scripts/compareResults.ts",
    "compare:dev": "npm run compare | pino-pretty",
    "build": "npm run clean && tsc -p tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "update-deps": "ncu -u && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ministryofjustice/bichard7-next-core.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-core/issues"
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-core#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/lodash.differencewith": "^4.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.orderby": "^4.6.6",
    "@types/node": "^17.0.23",
    "@types/nunjucks": "^3.2.1",
    "@types/promise-poller": "^1.7.1",
    "@types/stompit": "^0.26.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.17.0",
    "axios": "^0.26.1",
    "babel-loader": "^8.2.4",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-xml-matcher": "^1.2.0",
    "lint-staged": "^12.3.7",
    "lodash.merge": "^4.6.2",
    "lodash.orderby": "^4.6.0",
    "mockdate": "^3.0.5",
    "npm-check-updates": "^12.5.4",
    "nunjucks": "^3.2.3",
    "pg-promise": "^10.11.1",
    "pino-pretty": "^7.6.0",
    "prettier": "^2.6.1",
    "promise-poller": "^1.9.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "@moj-bichard7-developers/bichard7-next-data": "^2.0.7",
    "date-fns": "^2.28.0",
    "fast-xml-parser": "^4.0.7",
    "lodash.differencewith": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "pino": "^7.9.2",
    "stompit": "^1.0.0",
    "zod": "^3.14.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix"
  }
}
