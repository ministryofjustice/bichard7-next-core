{
  "updateTime": 1693922094738,
  "updatedBy": "Workflow file: f5a9274c30e4b87e, Commit hash: 8d4e38f3",
  "accessPolicy": {},
  "name": "incoming_message_handler",
  "description": "Runs phase 1 processing for Bichard in Core",
  "version": 4,
  "tasks": [
    {
      "name": "extract_paths",
      "taskReferenceName": "extract_paths",
      "inputParameters": {
        "records": "${workflow.input.records}",
        "queryExpression": ".records[0].s3.object.key"
      },
      "type": "JSON_JQ_TRANSFORM",
      "startDelay": 0,
      "optional": false,
      "asyncComplete": false
    },
    {
      "name": "convert_spi_to_aho",
      "taskReferenceName": "convert_spi_to_aho",
      "inputParameters": {
        "s3Path": "${extract_paths.output.result}"
      },
      "type": "SIMPLE",
      "startDelay": 0,
      "optional": false,
      "asyncComplete": false
    },
    {
      "name": "create_audit_log_record",
      "taskReferenceName": "create_audit_log_record",
      "inputParameters": {
        "auditLogRecord": "${convert_spi_to_aho.output.auditLogRecord}"
      },
      "type": "SIMPLE",
      "startDelay": 0,
      "optional": false,
      "asyncComplete": false
    },
    {
      "name": "parsing_failure",
      "taskReferenceName": "parsing_failure",
      "inputParameters": {
        "error": "${convert_spi_to_aho.output.error}"
      },
      "type": "SWITCH",
      "decisionCases": {
        "parsing_failed": [
          {
            "name": "store_audit_log_events",
            "taskReferenceName": "record_parsing_failure",
            "inputParameters": {
              "correlationId": "${convert_spi_to_aho.output.correlationId}",
              "auditLogEvents": "${convert_spi_to_aho.output.auditLogEvents}"
            },
            "type": "SIMPLE",
            "startDelay": 0,
            "optional": false,
            "asyncComplete": false
          },
          {
            "name": "alert_common_platform",
            "taskReferenceName": "alert_common_platform",
            "inputParameters": {
              "errorReportData": "${convert_spi_to_aho.output.errorReportData}"
            },
            "type": "SIMPLE",
            "startDelay": 0,
            "optional": false,
            "asyncComplete": false
          }
        ]
      },
      "defaultCase": [
        {
          "name": "duplicate_message",
          "taskReferenceName": "duplicate_message",
          "inputParameters": {
            "duplicateMessage": "${create_audit_log_record.output.duplicateMessage}"
          },
          "type": "SWITCH",
          "decisionCases": {
            "isDuplicate": [
              {
                "name": "store_audit_log_events",
                "taskReferenceName": "record_duplicate_message",
                "inputParameters": {
                  "correlationId": "${create_audit_log_record.output.duplicateCorrelationId}",
                  "auditLogEvents": "${create_audit_log_record.output.auditLogEvents}"
                },
                "type": "SIMPLE",
                "startDelay": 0,
                "optional": false,
                "asyncComplete": false
              }
            ]
          },
          "defaultCase": [
            {
              "name": "begin_processing",
              "taskReferenceName": "begin_processing",
              "inputParameters": {
                "startWorkflow": {
                  "correlationId": "${convert_spi_to_aho.output.correlationId}",
                  "input": {
                    "s3TaskDataPath": "${convert_spi_to_aho.output.s3TaskDataPath}"
                  },
                  "name": "bichard_process"
                }
              },
              "type": "START_WORKFLOW",
              "startDelay": 0,
              "optional": false,
              "asyncComplete": false
            }
          ],
          "startDelay": 0,
          "optional": false,
          "asyncComplete": false,
          "evaluatorType": "value-param",
          "expression": "duplicateMessage"
        }
      ],
      "startDelay": 0,
      "optional": false,
      "asyncComplete": false,
      "evaluatorType": "value-param",
      "expression": "error"
    }
  ],
  "inputParameters": ["s3Path"],
  "outputParameters": {
    "s3TaskDataPath": "${convert_spi_to_aho.output.s3TaskDataPath}"
  },
  "schemaVersion": 2,
  "restartable": true,
  "workflowStatusListenerEnabled": false,
  "timeoutPolicy": "ALERT_ONLY",
  "timeoutSeconds": 0,
  "variables": {},
  "inputTemplate": {}
}
