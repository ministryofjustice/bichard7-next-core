{
  "name": "handle_incoming_messages",
  "event": "sqs:incomingMessageQueue",
  "active": true,
  "condition": "!!$.detail.object.key",
  "actions": [
    {
      "action": "start_workflow",
      "start_workflow": {
        "name": "incoming_message_handler",
        "input": { "s3Path": "${detail.object.key}" }
      }
    }
  ]
}
