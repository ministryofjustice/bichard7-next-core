{
  "name": "@moj-bichard7/core",
  "version": "2.0.1",
  "description": "The core processing logic of Bichard 7",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf ./dist",
    "watch": "tsc --project tsconfig.build.json --watch",
    "build:esbuild": "node build.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest --runInBand --forceExit --testNamePattern=",
    "test:unit": "jest --testPathIgnorePatterns='characterisation-tests|comparison.test|integration.test|e2e.test' --maxWorkers=80% --testNamePattern=",
    "test:unit:dev": "jest --testPathIgnorePatterns='characterisation-tests|comparison.test|integration.test|e2e.test' --workerThreads=true --testNamePattern=",
    "test:unit:coverage": "jest --testPathIgnorePatterns='characterisation-tests|comparison.test|integration.test|e2e.test' --maxWorkers=80% --coverage=true ",
    "test:update": "jest -u --testPathIgnorePatterns='characterisation-tests|comparison.test|integration.test' 'integration.test'",
    "test:bichard": "USE_BICHARD=true jest --runInBand --testPathIgnorePatterns='characterisation-tests|comparison.test|integration.test' --workerThreads=true --testNamePattern=",
    "test:watch": "jest --watch --testPathIgnorePatterns='characterisation-tests|comparison.test|integration.test' --workerThreads=true",
    "test:compare": "NODE_ENV=test ts-node -T comparison/cli.ts --phase 1 -d ./phase1/tests/fixtures/e2e-comparison",
    "test:compare:e2e": "jest --runInBand --forceExit --testPathPattern=e2e.comparison.test --workerThreads=true",
    "test:integration": "jest --forceExit --runInBand --testPathPattern=integration.test --workerThreads=true",
    "test:server": "NODE_ENV=test ts-node scripts/testServer.ts",
    "test:e2e": "docker-compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-worker.yml -f environment/docker-compose-e2e-tests.yml run e2e-tests npm run test:chunk",
    "test:e2e:ci": "docker-compose --project-name bichard -f environment/docker-compose.yml -f environment/docker-compose-e2e-tests.yml run e2e-tests npm run test:chunk",
    "test:characterisation:bichard": "cd characterisation-tests && USE_BICHARD=true  jest --runInBand --workerThreads=true",
    "test:characterisation:bichard:phase1": "ENABLE_PHASE_2=false ENABLE_PHASE_3=false npm run test:characterisation:bichard",
    "test:characterisation:bichard:phase2": "ENABLE_PHASE_2=true ENABLE_PHASE_3=false npm run test:characterisation:bichard",
    "test:characterisation": "cd characterisation-tests && jest --runInBand --workerThreads=true",
    "compare": "TS_NODE_TRANSPILE_ONLY=true ts-node comparison/cli.ts",
    "compare:files": "scripts/files-from-comparison.js",
    "compare:summarise": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/summariseRecord.ts",
    "compare:summarise:extended": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/extendedSummary.ts",
    "compare:repeat": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/repeatTestBichard.ts",
    "compare:skip": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/skip-test.ts",
    "compare:runonbichard": "TS_NODE_TRANSPILE_ONLY=true ts-node scripts/runFileOnBichard.ts",
    "update-deps": "ncu -u && npm install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ministryofjustice/bichard7-next-core.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ministryofjustice/bichard7-next-core/issues"
  },
  "exports": {
    "./*": "./dist/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "homepage": "https://github.com/ministryofjustice/bichard7-next-core#readme",
  "devDependencies": {
    "@types/diff": "^5.2.3",
    "@types/jest": "^29.5.13",
    "@types/lodash.differencewith": "^4.5.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.merge": "^4.6.9",
    "@types/lodash.orderby": "^4.6.9",
    "@types/node": "^22.7.5",
    "@types/nunjucks": "^3.2.6",
    "@types/pretty-data": "^0.40.3",
    "@types/promise-poller": "^1.7.4",
    "@types/s3rver": "^3.7.4",
    "@types/stompit": "^0.26.6",
    "chalk": "^4.1.2",
    "esbuild": "^0.21.5",
    "esbuild-plugin-ignore": "^1.1.1",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "jest-mock-server": "^0.1.0",
    "jest-xml-matcher": "^1.2.0",
    "lodash.ismatch": "^4.4.0",
    "lodash.merge": "^4.6.2",
    "mailhog": "^4.16.0",
    "mockdate": "^3.0.5",
    "nunjucks": "^3.2.4",
    "pg-promise": "^11.10.1",
    "promise-poller": "^1.9.1",
    "s3rver": "^3.7.1",
    "ts-command-line-args": "^2.5.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.670.0",
    "@aws-sdk/client-s3": "^3.670.0",
    "@io-orkes/conductor-javascript": "^2.1.2",
    "@types/lodash.ismatch": "^4.4.9",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "bichard7-next-data-latest": "npm:@moj-bichard7-developers/bichard7-next-data@^2.0.215",
    "diff": "^7.0.0",
    "dynalite": "^3.2.2",
    "fast-xml-parser": "^4.5.0",
    "lodash.differencewith": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "p-limit": "^3.1.0",
    "postgres": "^3.4.4",
    "pretty-data": "github:vkiryukhin/pretty-data#22a34a534745714ea974f72db2e9bf7561b3fdc7",
    "stompit": "^1.0.0",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.4.0"
  }
}
