{
  "name": "@moj-bichard7/ui",
  "version": "0.1.0",
  "scripts": {
    "preinstall:assets": "./scripts/symlink-node-modules.sh",
    "install:assets": "./scripts/copy-govuk-frontend-assets.sh && ./scripts/copy-moj-frontend-assets.sh",
    "predev": "./scripts/symlink-node-modules.sh",
    "dev": "next dev -p 4080",
    "prebuild": "./scripts/symlink-node-modules.sh",
    "build": "next build",
    "build:analyse": "ANALYZE=true npm run build",
    "build:docker": "cd ../.. && bash scripts/build-ui-docker.sh",
    "start": "next start -p 4080",
    "prelint": "./scripts/symlink-node-modules.sh",
    "lint": "eslint .",
    "prelint:fix": "./scripts/symlink-node-modules.sh",
    "lint:fix": "eslint . --fix",
    "cypress:open": "cypress open",
    "cypress:open:docker": "cypress open --config baseUrl='https://localhost:4443'",
    "cypress:run": "cypress run",
    "cypress:run:docker": "cypress run --config baseUrl='https://localhost:4443'",
    "cypress:run:component": "cypress run --component",
    "cypress:run:component:docker": "cypress run --component --config baseUrl='https://localhost:4443'",
    "cypress:run:component:docker:ms-edge": "cypress run --browser edge --component --config baseUrl='https://localhost:4443'",
    "cypress:run:file": "cypress run --spec",
    "cypress:run:docker:file": "cypress run --config baseUrl='https://localhost:4443' --spec",
    "test": "npm run test:unit && npm run test:integration && npm run test:ui:e2e",
    "test:unit": "jest --testPathIgnorePatterns=\"integration\" \"e2e\" --maxWorkers=80%",
    "test:unit:dev": "jest --testPathIgnorePatterns=\"integration\" \"e2e\" --runInBand --workerThreads=true",
    "test:unit:update": "npm run test:unit -- -u",
    "test:integration": "jest --testPathPatterns=\"integration\" --runInBand --workerThreads=true --testNamePattern=",
    "test:integration:update": "npm run test:integration -- -u",
    "test:file": "jest --runInBand --workerThreads=true --testPathPatterns",
    "test:ui:e2e": "concurrently --kill-others --success first \"npm run start\" \"npm run cypress:run\"",
    "install:hooks": "./scripts/install-git-hooks.sh",
    "update-deps": "ncu -u && npm install",
    "seed-data": "ts-node ./scripts/seed-data.ts",
    "zap": "./scripts/zap/zap-scan.sh"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix --max-warnings 0 --no-warn-ignored --ignore-pattern !.ncurc.js",
    "*.scss": "stylelint './styles/*.scss' --fix"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^7.1.0",
    "@moj-bichard7-developers/bichard7-next-data": "^2.0.309",
    "axios": "^1.13.1",
    "cookie": "^1.0.2",
    "cookie-signature": "^1.2.2",
    "cookies-next": "4.3.0",
    "csrf": "^3.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "downshift": "^9.0.10",
    "govuk-frontend": "^5.13.0",
    "json-2-csv": "^5.5.10",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "next": "15.5.5",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "qs": "^6.14.0",
    "raw-body": "^3.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reflect-metadata": "^0.2.2",
    "stompit": "^1.0.0",
    "styled-components": "^6.1.19",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@jest/globals": "^30.2.0",
    "@next/bundle-analyzer": "^16.0.1",
    "@next/eslint-plugin-next": "^16.0.1",
    "@stdlib/random-base-exponential": "^0.2.1",
    "@stdlib/random-sample": "^0.2.1",
    "@swc/core": "^1.14.0",
    "@swc/jest": "^0.2.39",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/cookie-signature": "^1.1.2",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.sample": "^4.2.9",
    "@types/node": "24.9.2",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/stompit": "^0.26.6",
    "@types/styled-components": "^5.1.35",
    "aws-sdk": "^2.1692.0",
    "axe-core": "^4.11.0",
    "concurrently": "^9.2.1",
    "cypress": "^14.5.4",
    "cypress-axe": "^1.7.0",
    "cypress-circleci-reporter": "^0.3.0",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^9.37.0",
    "eslint-config-next": "^16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mocha": "^11.2.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-prettier": "^5.5.4",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "lint-staged": "^16.2.6",
    "lodash.sample": "^4.2.1",
    "mocha": "^11.7.4",
    "mockdate": "^3.0.5",
    "npm-check-updates": "^19.1.2",
    "pg-promise": "^12.2.0",
    "postgres": "^3.4.7",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "5.9.3",
    "wait-on": "^9.0.1",
    "webpack": "^5.102.1"
  }
}
