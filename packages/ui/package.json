{
  "name": "@moj-bichard7/ui",
  "version": "0.1.0",
  "scripts": {
    "preinstall:assets": "./scripts/symlink-node-modules.sh",
    "install:assets": "./scripts/copy-govuk-frontend-assets.sh && ./scripts/copy-moj-frontend-assets.sh",
    "predev": "./scripts/symlink-node-modules.sh",
    "dev": "next dev -p 4080",
    "prebuild": "./scripts/symlink-node-modules.sh",
    "build": "next build",
    "start": "next start -p 4080",
    "prelint": "./scripts/symlink-node-modules.sh",
    "lint": "eslint .",
    "prelint:fix": "./scripts/symlink-node-modules.sh",
    "lint:fix": "eslint . --fix",
    "cypress:open": "cypress open",
    "cypress:open:docker": "cypress open --config baseUrl='https://localhost:4443'",
    "cypress:run": "cypress run",
    "cypress:run:docker": "cypress run --config baseUrl='https://localhost:4443'",
    "cypress:run:component": "cypress run --component",
    "cypress:run:component:docker": "cypress run --component --config baseUrl='https://localhost:4443'",
    "cypress:run:component:docker:ms-edge": "cypress run --browser edge --component --config baseUrl='https://localhost:4443'",
    "cypress:run:file": "cypress run --spec",
    "cypress:run:docker:file": "cypress run --config baseUrl='https://localhost:4443' --spec",
    "test": "npm run test:unit && npm run test:integration && npm run test:ui:e2e",
    "test:unit": "jest --testPathIgnorePatterns=\"integration\" \"e2e\" --maxWorkers=80%",
    "test:unit:dev": "jest --testPathIgnorePatterns=\"integration\" \"e2e\" --runInBand --workerThreads=true",
    "test:unit:update": "npm run test:unit -- -u",
    "test:integration": "jest --testPathPattern=\"integration\" --runInBand --workerThreads=true --testNamePattern=",
    "test:integration:update": "npm run test:integration -- -u",
    "test:file": "jest --runInBand --testPathPattern --workerThreads=true",
    "test:ui:e2e": "concurrently --kill-others --success first \"npm run start\" \"npm run cypress:run\"",
    "install:hooks": "./scripts/install-git-hooks.sh",
    "update-deps": "ncu -u && npm install",
    "seed-data": "ts-node ./scripts/seed-data.ts",
    "zap": "./scripts/zap/zap-scan.sh"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix --max-warnings 0 --ignore-pattern !.ncurc.js",
    "*.scss": "stylelint './styles/*.scss' --fix"
  },
  "dependencies": {
    "@govuk-react/select": "^0.10.7",
    "@govuk-react/table": "^0.10.7",
    "@ministryofjustice/frontend": "^3.0.2",
    "@moj-bichard7-developers/bichard7-next-data": "^2.0.227",
    "@types/lodash.clonedeep": "^4.5.9",
    "axios": "^1.7.7",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cookie": "^1.0.1",
    "cookie-signature": "^1.2.2",
    "cookies-next": "^5.0.2",
    "csrf": "^3.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "date-time-format-timezone": "^1.0.22",
    "downshift": "^9.0.8",
    "govuk-frontend": "^5.7.1",
    "govuk-react": "^0.10.7",
    "json-2-csv": "^5.5.6",
    "lodash": "^4.17.21",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isobject": "^3.0.2",
    "next": "15.0.3",
    "pg": "^8.13.1",
    "pino": "^9.5.0",
    "qs": "^6.13.0",
    "raw-body": "^3.0.0",
