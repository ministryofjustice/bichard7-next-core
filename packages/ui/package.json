    "@types/stompit": "^0.26.6",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "aws-sdk": "^2.1682.0",
    "axe-core": "^4.10.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^8.2.2",
    "cypress": "^13.13.3",
    "cypress-axe": "^1.5.0",
    "cypress-circleci-reporter": "^0.2.0",
    "http-server": "^14.1.1",
    "jest-axe": "^9.0.0",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.9",
    "lodash.frompairs": "^4.0.1",
    "lodash.sample": "^4.2.1",
    "mocha": "^10.7.3",
    "mockdate": "^3.0.5",
    "npm-check-updates": "^17.1.0",
    "pg-promise": "^11.9.1",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.8.2",
    "stylelint-config-standard-scss": "^13.1.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.5.4",
    "uuid": "^10.0.0",
    "wait-on": "^8.0.0"
  }
}
