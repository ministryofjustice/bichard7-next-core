{
  "name": "@moj-bichard7/user-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "./scripts/symlink-node-modules.sh",
    "dev": "COOKIES_SECURE=false BASE_URL=http://localhost:3000 next dev",
    "prebuild": "./scripts/symlink-node-modules.sh",
    "build": "next build",
    "build:docker": "cd ../.. && bash scripts/build-user-service-docker.sh",
    "preinstall:assets": "./scripts/symlink-node-modules.sh",
    "install:assets": "./scripts/copy-govuk-frontend-assets.sh && ./scripts/copy-moj-frontend-assets.sh",
    "install:hooks": "./scripts/install-git-hooks.sh",
    "start": "COOKIES_SECURE=false next start",
    "start:prod": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:file": "cypress run --spec",
    "test:unit": "jest --testPathIgnorePatterns=\"integration\" \"e2e\" --runInBand --env='jsdom'",
    "test:unit:update": "npm run test:unit -- -u",
    "test:integration": "jest --testPathPatterns=\"integration\" --runInBand",
    "test:integration:update": "npm run test:integration -- -u",
    "test:ui": "./scripts/run-test-ui.sh",
    "update-deps": "ncu -u && npm install"
  },
  "dependencies": {
    "@ministryofjustice/frontend": "^5.1.5",
    "@moj-bichard7-developers/bichard7-next-data": "^2.0.298",
    "@trust/webcrypto": "^0.9.2",
    "argon2": "^0.44.0",
    "axios": "^1.11.0",
    "badwords-list": "^1.0.0",
    "classnames": "^2.5.1",
    "cookie": "^1.0.2",
    "cookie-signature": "^1.2.2",
    "cookies-next": "^6.1.0",
    "crypto-secure-random-digit": "^1.0.10",
    "csrf": "^3.1.0",
    "date-fns": "^4.1.0",
    "govuk-frontend": "^5.11.2",
    "govuk-react": "^0.10.7",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.5.3",
    "nodemailer": "^7.0.6",
    "pg": "^8.16.3",
    "pg-promise": "^12.1.3",
    "pino": "^9.9.0",
    "qs": "^6.14.0",
    "raw-body": "^3.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-jss": "^10.10.0",
    "styled-components": "^6.1.19",
    "uuid": "^11.1.0",
    "word-list": "^3.1.0"
  },
  "devDependencies": {
    "@swc/core": "^1.13.19",
    "@swc/jest": "^0.2.39",
    "@testing-library/react": "^16.3.0",
    "@types/cookie": "^1.0.0",
    "@types/cookie-signature": "^1.1.2",
    "@types/csrf": "^3.1.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.1",
    "@types/qs": "^6.14.0",
    "@types/random-words": "^1.2.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.1",
    "cypress": "^15.0.0",
    "jest": "^30.1.2",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.5",
    "npm-check-updates": "^18.0.3",
    "sass": "^1.91.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix --max-warnings 0 --ignore-pattern !.ncurc.js"
  }
}
